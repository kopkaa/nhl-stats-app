{
	"env": {
			"browser": true,
			"commonjs": true,
			"es2021": true
	},
	"extends": [
			"airbnb-base",
			"plugin:vue/recommended"
	],
	"parserOptions": {
			"ecmaVersion": 12
	},

	"rules": {
		"quotes": [2, "single"],
		"space-before-function-paren": ["error", "always"],
		"brace-style": [2, "stroustrup"]
	}
}